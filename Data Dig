Display Astronaut Data
SELECT * FROM astronauts;

Display astronauts after 1950
SELECT Name FROM astronauts where Year > 1950;

What is the average space flight hour of the astronauts
Select avg(Space_Flight_hr) FROM astronauts;

What is the maximum space flight hour
SELECT max(Space_Flight_hr) FROM astronauts;

What is the minimum space flight hour
SELECT min(Space_Flight_hr) FROM astronauts;

Disply active male astronauts
SELECT Name FROM astronauts WHERE Status = "Active" and Gender = "Male";

Display active female astronauts
SELECT Name FROM astronauts WHERE Status = "Active" and Gender = "Female";

Display retired astronauts
SELECT Name FROM astronauts WHERE Status = "Retired";

Display astronauts who majored in physics or engineering whether grad or undergrad
SELECT Name FROM astronauts WHERE Graduate_Major
LIKE("engineering") or Undergraduate_Major LIKE("physics");

Display total flights of each astronauts
SELECT Name, Space_Flights AS total_flights FROM astronauts;

Display astronauts whose total flights are more than two
SELECT Name, Space_Flights AS total_flights FROM astronauts GROUP BY Name HAVING total_flights > 2;

Displays astronauts grouped by generation
SELECT *,
    CASE
        WHEN Birth_Year > 1901 THEN "GI_Generation"
        WHEN Birth_Year > 1927 THEN "Silent_Generation"
        WHEN Birth_Year > 1945 THEN "Boomer_Generation"
        WHEN Birth_Year > 1964 THEN "Gen_X"
        WHEN Birth_Year > 1980 THEN "Millenial"
        WHEN Birth_Year > 1996 THEN "Gen_z"
        END AS "Generations"
FROM astronauts;
